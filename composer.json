{
  "name": "lokalise/php-lokalise-api",
  "type": "library",
  "description": "Lokalise API",
  "homepage": "https://lokalise.co/docs/php/index.html",
  "keywords": ["lokalise", "api"],
  "license": "MIT",
  "authors": [
    {
      "name": "Lokalise team",
      "homepage": "https://lokalise.co"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5 || ^6.0",
    "php-http/guzzle6-adapter": "^1.0",
    "php-http/mock-client": "^1.0",
    "guzzlehttp/psr7": "^1.2"
  },
  "autoload": {
    "psr-4": { "Github\\": "lib/Github/" }
  },
  "autoload-dev": {
    "psr-4": { "Github\\Tests\\": "test/Github/Tests/"}
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.x.x-dev"
    }
  }
}
