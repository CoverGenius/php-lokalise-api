{
  "name": "lokalise/php-lokalise-api",
  "type": "library",
  "description": "Lokalise API client",
  "homepage": "https://lokalise.co/api2docs/php/",
  "keywords": ["REST", "API", "Lokalise"],
  "license": "MIT",
  "authors": [
    {
      "name": "Lokalise team",
      "homepage": "https://lokalise.co"
    }
  ],
  "require": {
    "php": "^5.6 || ~7.0",
    "php-http/guzzle6-adapter": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6 || ^6.0",
    "php-http/mock-client": "^1.0",
    "guzzlehttp/psr7": "^1.2"
  },
  "autoload": {
    "psr-4": { "Lokalise\\": "Api/" }
  },
  "autoload-dev": {
    "psr-4": { "Lokalise\\Tests\\": "Tests/"}
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.x.x-dev"
    }
  }
}
